{
  "name": "quizzz",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.1",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "moment": "^2.29.1",
    "node-sass": "^4.14.1",
    "react": "^17.0.2",
    "react-alert": "^7.0.3",
    "react-alert-template-basic": "^1.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0",
    "web-vitals": "^1.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "drf": "cd drf/ && ${npm_config_drf_venv}/python manage.py runserver localhost:5000",
    "drf-coverage": "cd drf/; ${npm_config_drf_venv}/coverage run --source='.' manage.py test; ${npm_config_drf_venv}/coverage report -m",
    "drf-db": "cd drf/; DRF_VENV=${npm_config_drf_venv} bash dev-db.sh",
    "deploy-staging": "cd deploy/ansible && ansible-playbook -i envs/staging/inventory.yml main.yml; cd ../..",
    "deploy-production": "cd deploy/ansible && ansible-playbook -i envs/production/inventory.yml main.yml -K; cd ../.."
  },
  "proxy": "http://localhost:5000",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "redux-devtools": "^3.7.0"
  }
}
